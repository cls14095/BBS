  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program(development)",
      "program": "${workspaceFolder}/app.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program(production)",
      "program": "${workspaceFolder}/app.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Gulp",
      "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
    }
  ]
}